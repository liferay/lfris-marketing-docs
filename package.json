	"version": "1.0.2",
	"author": "Diego Nascimento <diego.nascimento@liferay.com",
	"bugs": {
		"url": "https://github.com/diegonvs/gatsby-boilerplate"
	},
	"homepage": "https://gatsby-boilerplate.wedeploy.io#readme",
